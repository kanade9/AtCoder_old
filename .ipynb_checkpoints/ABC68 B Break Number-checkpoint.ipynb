    "import math\n",
    "n=int(input())\n",
    "wari=0\n",
    "ans=0\n",
    "for i in range(1,n+1):\n",
    "    tmp=int(math.log2(i))\n",
    "    if tmp>wari:\n",
    "        wari=tmp\n",
    "        ans=i\n",
    "print(ans if n!=1 else 1)"
